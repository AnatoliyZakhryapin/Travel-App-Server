{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Debug"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "your_new_secret_key_that_is_longer_and_secure",
    "Issuer": "your_app",
    "Audience": "your_audience",
    "ExpiryMinutes": 60
  }
}